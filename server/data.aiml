<aiml> 

    <category>
        <pattern>HI *</pattern>
        <template>Hello. What is your name?</template>
    </category>

   <category>
        <pattern>HELLO *</pattern>
        <template>Hello. What is your name?</template>
    </category>

    <category>
        <pattern>WHAT IS YOUR NAME *</pattern>
        <template>My name is <bot name='name'/>.</template>
    </category>
    
    <!-- <category>

        <pattern>MY NAME IS *</pattern>
        <think><set name="clientName"><star/></set></think>
        <template>Name -> <get name="clientName"/></template>
    </category> -->

    <category>
        <pattern><set name='name'>!NONAME</set></pattern>
        <template>Name reset=> <get name='name'/></template>
    </category>

        <category>
        <pattern><set name='age'>!NOAGE</set></pattern>
        <template>Age reset=> <get name='name'/></template>
    </category>

    <category>
        <pattern><set name='job'>!NOJOB</set></pattern>
        <template>Job reset=> <get name='name'/></template>
    </category>

    <category>
        <pattern>MY NAME IS <set name='name'>*</set></pattern>
        <template>Hey <get name='name'/>!</template>
    </category>

    <category>
        <pattern>REMEMBER ME?</pattern>
        <template>
            <condition name='name'>
                <li value='!noname'>No, what is your name?</li>
                <li>Hi, <get name='name'/></li>      
            </condition>
        </template>
    </category>
</aiml>